{
  "colorScheme": {
    "primary": {
      "50": "#f0f7ff",
      "100": "#e0efff",
      "200": "#b9dfff",
      "300": "#7cc4ff",
      "400": "#36a5ff",
      "500": "#0d84ff",
      "600": "#0066ff",
      "700": "#164c78",
      "800": "#0f3a5a",
      "900": "#0a2642"
    },
    "complementary": {
      "50": "#fff8f0",
      "100": "#fff0e0",
      "200": "#ffdeb9",
      "300": "#ffc47c",
      "400": "#ffa536",
      "500": "#ff840d",
      "600": "#ff6600",
      "700": "#785016",
      "800": "#5a3c0f",
      "900": "#42290a"
    },
    "neutrals": {
      "white": "#ffffff",
      "gray": {
        "50": "#f9fafb",
        "100": "#f3f4f6",
        "200": "#e5e7eb",
        "300": "#d1d5db",
        "400": "#9ca3af",
        "500": "#6b7280",
        "600": "#4b5563",
        "700": "#374151",
        "800": "#1f2937",
        "900": "#111827"
      },
      "black": "#000000"
    },
    "semantic": {
      "success": {
        "50": "#f0fdf4",
        "500": "#22c55e",
        "700": "#15803d"
      },
      "warning": {
        "50": "#fffbeb",
        "500": "#f59e0b",
        "700": "#b45309"
      },
      "error": {
        "50": "#fef2f2",
        "500": "#ef4444",
        "700": "#b91c1c"
      },
      "info": {
        "50": "#eff6ff",
        "500": "#3b82f6",
        "700": "#1d4ed8"
      }
    }
  },
  "typography": {
    "fontFamilies": {
      "heading": "Inter, system-ui, sans-serif",
      "body": "Inter, system-ui, sans-serif",
      "mono": "JetBrains Mono, Consolas, monospace"
    },
    "fontSizes": {
      "xs": "0.75rem",
      "sm": "0.875rem",
      "base": "1rem",
      "lg": "1.125rem",
      "xl": "1.25rem",
      "2xl": "1.5rem",
      "3xl": "1.875rem",
      "4xl": "2.25rem",
      "5xl": "3rem",
      "6xl": "3.75rem"
    },
    "fontWeights": {
      "light": 300,
      "normal": 400,
      "medium": 500,
      "semibold": 600,
      "bold": 700,
      "extrabold": 800
    },
    "lineHeights": {
      "tight": 1.25,
      "normal": 1.5,
      "relaxed": 1.75
    }
  },
  "spacing": {
    "xs": "0.25rem",
    "sm": "0.5rem",
    "md": "1rem",
    "lg": "1.5rem",
    "xl": "2rem",
    "2xl": "3rem",
    "3xl": "4rem",
    "4xl": "6rem",
    "5xl": "8rem"
  },
  "borderRadius": {
    "none": "0",
    "sm": "0.25rem",
    "base": "0.375rem",
    "md": "0.5rem",
    "lg": "0.75rem",
    "xl": "1rem",
    "2xl": "1.5rem",
    "full": "9999px"
  },
  "shadows": {
    "sm": "0 1px 2px 0 rgba(0, 0, 0, 0.05)",
    "base": "0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06)",
    "md": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)",
    "lg": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05)",
    "xl": "0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 10px 10px -5px rgba(0, 0, 0, 0.04)",
    "2xl": "0 25px 50px -12px rgba(0, 0, 0, 0.25)"
  },
  "components": {
    "button": {
      "primary": {
        "background": "primary.700",
        "color": "white",
        "hoverBackground": "primary.800",
        "activeBackground": "primary.900",
        "borderRadius": "md",
        "padding": "md lg",
        "fontWeight": "semibold",
        "shadow": "base"
      },
      "secondary": {
        "background": "complementary.500",
        "color": "white",
        "hoverBackground": "complementary.600",
        "activeBackground": "complementary.700",
        "borderRadius": "md",
        "padding": "md lg",
        "fontWeight": "semibold",
        "shadow": "base"
      },
      "outline": {
        "background": "transparent",
        "color": "primary.700",
        "border": "2px solid primary.700",
        "hoverBackground": "primary.50",
        "activeBackground": "primary.100",
        "borderRadius": "md",
        "padding": "md lg",
        "fontWeight": "semibold"
      },
      "ghost": {
        "background": "transparent",
        "color": "primary.700",
        "hoverBackground": "primary.50",
        "activeBackground": "primary.100",
        "borderRadius": "md",
        "padding": "md lg",
        "fontWeight": "medium"
      }
    },
    "card": {
      "default": {
        "background": "white",
        "border": "1px solid neutrals.gray.200",
        "borderRadius": "xl",
        "padding": "xl",
        "shadow": "base"
      },
      "elevated": {
        "background": "white",
        "borderRadius": "xl",
        "padding": "xl",
        "shadow": "lg"
      },
      "outlined": {
        "background": "white",
        "border": "2px solid neutrals.gray.200",
        "borderRadius": "xl",
        "padding": "xl"
      }
    },
    "navigation": {
      "navbar": {
        "background": "rgba(255, 255, 255, 0.95)",
        "backdropFilter": "blur(20px)",
        "borderBottom": "1px solid neutrals.gray.200",
        "padding": "md xl",
        "shadow": "sm"
      },
      "link": {
        "color": "neutrals.gray.700",
        "hoverColor": "primary.700",
        "activeColor": "primary.800",
        "fontWeight": "medium"
      }
    },
    "input": {
      "default": {
        "background": "white",
        "border": "1px solid neutrals.gray.300",
        "borderRadius": "md",
        "padding": "md",
        "focusBorder": "2px solid primary.500",
        "focusRing": "0 0 0 3px rgba(22, 76, 120, 0.1)"
      }
    },
    "badge": {
      "primary": {
        "background": "primary.100",
        "color": "primary.800",
        "borderRadius": "full",
        "padding": "xs sm",
        "fontSize": "sm",
        "fontWeight": "medium"
      },
      "complementary": {
        "background": "complementary.100",
        "color": "complementary.800",
        "borderRadius": "full",
        "padding": "xs sm",
        "fontSize": "sm",
        "fontWeight": "medium"
      },
      "success": {
        "background": "semantic.success.50",
        "color": "semantic.success.700",
        "borderRadius": "full",
        "padding": "xs sm",
        "fontSize": "sm",
        "fontWeight": "medium"
      }
    }
  },
  "layout": {
    "container": {
      "maxWidth": "1200px",
      "padding": "0 1.5rem",
      "margin": "0 auto"
    },
    "section": {
      "paddingY": "4xl",
      "paddingX": "lg"
    },
    "breakpoints": {
      "sm": "640px",
      "md": "768px",
      "lg": "1024px",
      "xl": "1280px",
      "2xl": "1536px"
    }
  },
  "gradients": {
    "primary": "linear-gradient(135deg, primary.600 0%, primary.800 100%)",
    "primaryToComplementary": "linear-gradient(135deg, primary.700 0%, complementary.600 100%)",
    "subtle": "linear-gradient(135deg, primary.50 0%, complementary.50 100%)",
    "hero": "linear-gradient(135deg, rgba(22, 76, 120, 0.9) 0%, rgba(120, 80, 22, 0.9) 100%)"
  }
}
